{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SupervisorConfig": {
    "DeployDirectory": "./bin/TempDeploy/",
    "BuildDirectory": "./bin/TempBuild/",
    "SourceDirectory": "../.",
    "ConfigDirectory": "./bin/TempConfig/"
  },
  "FileLogging": {
    "PathFormat": "logs/{Date}.log",
    "LogLevel": {
      "Default": "Information",
      "System.Net.Http.HttpClient": "Warning"
    },
    "RetainedFileCountLimit": 30,
    "OutputTemplate": "{Timestamp:o} [{Level}] ({SourceContext}) {Message}{NewLine}{Exception}"
  }
}
